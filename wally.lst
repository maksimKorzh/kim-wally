;-----------------------------------------------------------------------;
;                               PRINT BOARD                             ;
;-----------------------------------------------------------------------;
  PBOARD:  0200  20 2F 1E  JSR $1E2F               ;                    ;
                 A2 00     LDX #$00                ;                    ;
  BCLOOP:        8A        TAX                     ;                    ;
                 E0 0A     CPX #$0A                ;                    ;
                 B0 0F     BCS ADDDGT              ;                    ;
                 90 12     BCC ADDCHR              ;                    ;
  PRNCOL:        20 A0 1E  JSR $1EA0               ;                    ;
                 20 9E 1E  JSR $1E9E               ;    Print column    ;
                 E8        INX                     ;                    ;
                 E0 10     CPX #$10                ;                    ;
                 D0 EE     BNE BCLOOP              ;                    ;
                 F0 0A     BEQ PRNPOS              ;                    ;
  ADDDGT:        69 36     ADD #$36                ;                    ;
                 4C 0C 02  JMP PRNCOL              ;                    ;
  ADDCHR:        69 30     ADD #$30                ;                    ;
                 4C 0C 02  JMP PRNCOL              ;                    ;
